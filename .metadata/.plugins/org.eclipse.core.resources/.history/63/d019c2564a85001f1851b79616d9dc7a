package com.hotel.entities;

import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import jakarta.persistence.Table;

@Entity
@Table(name = "HotelsData")
public class Hotels {

	@Id
	@Column(name = "HotelId")
	private String hotelId;
	@Column(name = "Name")
	private String name;
}
