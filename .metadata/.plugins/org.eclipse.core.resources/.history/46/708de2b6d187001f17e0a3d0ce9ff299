package com.hotel.services;

import java.util.List;

import com.hotel.entities.Staffs;

public interface StaffService {

	// Create
	Staffs create(Staffs staffs);

	// Update
	Staffs update(Staffs staffs);

	// get by hotel id
	Staffs get(String hotelId);

	// get all
	List<Staffs> getAll();

	// delete by staff id
	String delete(String staffId);

}
